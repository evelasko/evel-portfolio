    "@types/lodash-es": "^4.17.5",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@types/react-icons": "^3.0.0",
    "@types/react-image-gallery": "^1.0.4",
    "@types/sass": "^1.16.1",
    "@types/styled-components": "^5.1.14",
    "@types/three": "^0.133.1",
