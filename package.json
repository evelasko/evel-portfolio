{
  "name": "velasco-portfolio",
  "private": true,
  "description": "Enrique Velasco's Portfolio of Works",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "deploy": "dotenv -- bash -c 'netlify deploy --prod --dir public --site $NETLIFY_SITE_ID'",
    "setup:contentful": "dotenv -- bash -c 'contentful space import --update false --space-id $CONTENTFUL_SPACE_ID --management-token $CONTENTFUL_MANAGEMENT_TOKEN --content-file content-model.json'",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^13.4.0",
    "@contentful/rich-text-types": "^14.1.2",
    "@fontsource/heebo": "^4.5.0",
    "babel-plugin-styled-components": "^1.13.2",
    "dotenv": "^10.0.0",
    "framer-motion": "^4.1.17",
    "gatsby": "^3.12.0",
    "gatsby-background-image": "^1.5.3",
    "gatsby-image": "^3.11.0",
    "gatsby-image-background-slider": "^0.0.4",
    "gatsby-plugin-image": "^1.12.0",
    "gatsby-plugin-playground": "^1.0.6",
    "gatsby-plugin-react-helmet": "^4.12.0",
    "gatsby-plugin-react-i18next": "^1.1.1",
    "gatsby-plugin-robots-txt": "^1.6.9",
    "gatsby-plugin-sass": "^4.13.0",
    "gatsby-plugin-sharp": "^3.12.0",
    "gatsby-plugin-sitemap": "^3.3.0",
    "gatsby-plugin-styled-components": "^4.12.0",
    "gatsby-plugin-transition-link": "^1.20.5",
    "gatsby-source-contentful": "^5.12.0",
    "gatsby-source-filesystem": "^3.12.0",
    "gatsby-transformer-sharp": "^3.12.0",
    "gbimage-bridge": "^0.1.4",
    "glslCanvas": "^0.1.7",
    "gsap": "^3.7.1",
    "i18next": "^21.3.1",
    "lodash-es": "^4.17.21",
    "popmotion": "8.6.4",
    "proptypes": "^1.1.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^5.2.1",
    "react-i18next": "^11.12.0",
